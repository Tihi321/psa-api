{
  "name": "psa-api",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/psa-api.cjs.js",
  "module": "./dist/psa-api.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/psa-api.es.js",
      "require": "./dist/psa-api.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "rimraf": "4.4.1",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  },
  "license": "MIT"
}
